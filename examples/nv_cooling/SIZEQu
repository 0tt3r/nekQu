c     Dimension file to be included     
                                        
c     Arrays for Quantum Model calculations
      integer    maxTimeSteps,maxN,maxOp,maxE,lp,nid 
      parameter (maxTimeSteps = 100000) ! Max number of time steps
      parameter (maxN    = 2000 )       ! Max Hilbert space size 
      parameter (maxOp   = 30  )       ! Max number of operators
      parameter (maxE    = 1000 )       ! Max number of energy slices
      parameter (lp      = 4  )       ! Max number of processors

      COMMON /DIMN/
     $           NID

