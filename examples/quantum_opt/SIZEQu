c     Dimension file to be included     
                                        
c     Arrays for Quantum Model calculations
      integer    maxTimeSteps,maxN,maxOp,maxE,lp,nid,larnol 
      parameter (maxTimeSteps = 10000) ! Max number of time steps
      parameter (maxN    = 96 )       ! Max Hilbert space size 
      parameter (maxOp   = 30  )       ! Max number of operators
      parameter (maxE    = 1000 )       ! Max number of energy slices
      parameter (lp      = 2  )       ! Max number of processors
      parameter (larnol  = 0  )       ! Max arnoldi subspace size
      COMMON /DIMN/
     $           NID

