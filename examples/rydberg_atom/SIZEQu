c     Dimension file to be included     
                                        
c     Arrays for Quantum Model calculations
      integer    maxTimeSteps,maxN,maxOp,maxE,lp,nid,larnol 
      parameter (maxTimeSteps = 2000001) ! Max number of time steps
      parameter (maxN    = 1200 )       ! Max Hilbert space size 
      parameter (maxOp   = 60  )       ! Max number of operators
      parameter (maxE    = 300 )       ! Max number of energy slices
      parameter (lp      = 16  )       ! Max number of processors
      parameter (larnol = 0  )        ! Max arnoldi subspace size for exponential timestepping
      COMMON /DIMN/
     $           NID

