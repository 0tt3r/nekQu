c     Dimension file to be included     
                                        
c     Arrays for Quantum Model calculations
      integer    maxTimeSteps,maxN,maxOp,maxE,lp,nid,larnol 
      parameter (maxTimeSteps = 100000) ! Max number of time steps
      parameter (maxN    = 1696 )       ! Max Hilbert space size 
      parameter (maxOp   = 30  )       ! Max number of operators
      parameter (maxE    = 101 )       ! Max number of energy slices
      parameter (lp      = 4  )       ! Max number of processors
      parameter (larnol = 0  )        ! Max arnoldi subspace size for exponential timestepping
      COMMON /DIMN/
     $           NID

