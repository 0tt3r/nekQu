c     Dimension file to be included     
                                        
c     Arrays for Quantum Model calculations
      integer    maxTimeSteps,maxN,maxOp,maxE,lp,nid,larnol 
      parameter (maxTimeSteps = 100001) ! Max number of time steps
      parameter (maxN    = 150 )       ! Max Hilbert space size 
      parameter (maxOp   = 30  )       ! Max number of operators
      parameter (maxE    = 1 )       ! Max number of energy slices
      parameter (lp      = 1  )       ! Max number of processors
      parameter (larnol = 500  )        ! Max arnoldi subspace size for exponential timestepping
      COMMON /DIMN/
     $           NID

